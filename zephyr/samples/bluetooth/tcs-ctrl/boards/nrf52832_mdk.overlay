&i2c0 { 
	status = "okay";
	clock-frequency = <I2C_BITRATE_STANDARD>;
	sda-pin = <26>;
	scl-pin = <27>;
};

&i2c1 { 
	status = "okay";
	clock-frequency = <I2C_BITRATE_STANDARD>;
	sda-pin = <29>;
	scl-pin = <30>;
};


&uart0 {
	current-speed = <115200>;
//	tx-pin = <24>;
//	rx-pin = <23>;
	tx-pin = <20>;
	rx-pin = <19>;
};